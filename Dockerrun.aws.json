{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "roaslin/multi-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "roaslin/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client"
            ],
            "memory": 128
        }
    ]
}